{
  "private": true,
  "scripts": {
    "start": "cross-env  umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "build:dev": "cross-env UMI_ENV=dev umi build",
    "build:testing": "cross-env UMI_ENV=testing umi build",
    "build": "cross-env UMI_ENV=pro umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "test:coverage": "umi-test --coverage",
    "test:inspect-brk": "node --inspect-brk ./node_modules/.bin/umi test",
    "analyze": "cross-env ANALYZE=1 umi build",
    "serve": "serve ./dist",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less"
  },
  "gitHooks": {
    "commit-msg": "node ./node_modules/@umijs/fabric/dist/verifyCommit.js"
  },
  "lint-staged": {
    "*.less": "stylelint --syntax less",
    "*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.1.4",
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-card": "^1.13.0",
    "@ant-design/pro-descriptions": "^1.7.13",
    "@ant-design/pro-form": "^1.26.0",
    "@ant-design/pro-layout": "^6.19.1",
    "@ant-design/pro-skeleton": "^1.0.1",
    "@ant-design/pro-table": "^2.39.0",
    "@antv/data-set": "^0.11.7",
    "antd": "^4.16.0",
    "antd-linkage": "^1.1.1",
    "classnames": "^2.3.1",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "omit.js": "^2.0.2",
    "open-browser-webpack-plugin": "^0.0.5",
    "qs": "^6.10.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^4.3.1",
    "umi": "^3.4.22",
    "umi-plugin-keep-alive": "0.0.1-beta.17",
    "umi-request": "^1.3.9"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/jest": "^25.1.2",
    "@types/mockjs": "^1.0.3",
    "@umijs/fabric": "^2.5.2",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/preset-dumi": "^1.1.18",
    "@umijs/preset-react": "^1.8.8",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.3.4",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "puppeteer-core": "^5.5.0",
    "stylelint": "^13.13.1",
    "typescript": "^4.0.5",
    "umi-plugin-autotips-components": "^1.0.7"

  },
  "engines": {
    "node": ">=10.0.0"
  }
}
