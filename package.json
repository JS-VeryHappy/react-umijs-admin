{
  "name": "react-umijs-admin",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "cross-env  umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "build:dev": "cross-env UMI_ENV=dev umi build",
    "build:test": "cross-env UMI_ENV=test umi build",
    "build": "cross-env UMI_ENV=pro umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi test",
    "test:src": "umi test ./src",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "test:coverage": "umi-test --coverage",
    "test:inspect-brk": "node --inspect-brk ./node_modules/.bin/umi test",
    "analyze": "cross-env ANALYZE=1 umi build",
    "serve": "serve ./dist",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi"
  },
  "gitHooks": {
    "commit-msg": "node ./node_modules/@umijs/fabric/dist/verifyCommit.js"
  },
  "lint-staged": {
    "*.less": "stylelint --syntax less",
    "*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.1.17",
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-card": "^1.14.2",
    "@ant-design/pro-descriptions": "^1.8.4",
    "@ant-design/pro-form": "^1.30.0",
    "@ant-design/pro-layout": "^6.19.7",
    "@ant-design/pro-list": "^1.10.2",
    "@ant-design/pro-skeleton": "^1.0.1",
    "@ant-design/pro-table": "^2.43.0",
    "antd": "^4.16.5",
    "classnames": "^2.3.1",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "omit.js": "^2.0.2",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "umi": "^3.4.25",
    "umi-plugin-keep-alive": "0.0.1-beta.27",
    "umi-request": "^1.3.9"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/jest": "^26.0.23",
    "@types/mockjs": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@umijs/fabric": "^2.6.2",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/plugin-openapi": "^1.3.0",
    "@umijs/preset-dumi": "^1.1.20",
    "@umijs/preset-react": "^1.8.13",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.4.25",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "lint-staged": "^11.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.3.0",
    "puppeteer-core": "^9.1.1",
    "stylelint": "^13.13.1",
    "typescript": "^4.0.5",
    "umi-plugin-autotips-components": "^1.0.9"
  },
  "resolutions": {
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
